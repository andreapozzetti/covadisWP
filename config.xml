<?xml version='1.0' encoding='utf-8'?>
<widget id="com.covadis.covadis" version="0.0.1" xmlns="http://www.w3.org/ns/widgets"  xmlns:gap="http://phonegap.com/ns/1.0">
    <name>covadis</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
	<gap:plugin name="org.apache.cordova.device" version="0.2.12" />
	<gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
	<gap:plugin name="org.apache.cordova.globalization" version="0.3.1" />
	<gap:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
	<gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />
	<gap:plugin name="com.brodysoft.sqliteplugin" version="1.0.4" />
	<gap:plugin name="com.phonegap.plugins.pushplugin" version="2.4.0" />
	<gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
	<gap:plugin name="org.apache.cordova.media" version="0.2.13" />
	<icon src="icon.png" />
    <icon gap:platform="winphone" src="res/icons/windows-phone/icon-48.png" />
	<icon gap:platform="winphone" src="res/icons/windows-phone/icon-62-tile.png" />
    <icon gap:platform="winphone" gap:role="background" src="res/icons/windows-phone/icon-173-tile.png" />
    <gap:splash gap:platform="winphone" src="res/screen/windows-phone/screen-portrait.jpg" />
    <content src="index.html" />
    <access origin="*" />
</widget>
